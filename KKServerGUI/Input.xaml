<Window x:Class="KKServerGUI.Input"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KKServerGUI"
        mc:Ignorable="d"
        Title="Input" Height="136.459" Width="550" WindowStartupLocation="CenterScreen" ShowInTaskbar="False" ResizeMode="NoResize">
    <StackPanel>
        <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="TextBlock" Height="33" Margin="4,4,4,0"/>
        <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Margin="4,5"/>
        <Grid Height="28">
            <Button x:Name="button" Content="Cancel" HorizontalAlignment="Left" Margin="448,7,0,2" Width="75" Click="button_Click"/>
            <Button x:Name="button1" Content="OK" HorizontalAlignment="Left" Margin="356,7,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
        </Grid>
    </StackPanel>
</Window>
